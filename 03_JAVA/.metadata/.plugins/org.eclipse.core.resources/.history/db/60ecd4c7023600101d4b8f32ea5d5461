package chap08.section02.example01;
//실행 클래스
public class ChildApplication {
public static void main(String[] args) {
	
	Child child =new Child();
	Child2 child2=new Child2();
	
//다형성: 1)코드를 조금만 추가해서 다양한 결과를 나타낼 수 있는 기능
   Parent parent=child;
   parent.method();
   parent=child2;
   parent.method();
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}
