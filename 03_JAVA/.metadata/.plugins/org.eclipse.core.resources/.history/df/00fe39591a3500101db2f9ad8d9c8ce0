package chap06.section04;
//실행 클래스  (결과 확인용)
public class CalculatorApplication {
public static void main(String[] args) {
//    전역변수 사용법2(실행클래스에서)
//	  클래스명.전역변수
    //원주율을 만들어보자: 반지금 * 반지름 *3.14	
	double resulte=10*10*Calculator.pi;
	System.out.println(resulte);
	
//     전역변수 사용법2(실행클래스에서) 하나더!
//	   전역함수를 사용해보자
	  System.out.println(Calculator.plus(5, 10));
}
}
