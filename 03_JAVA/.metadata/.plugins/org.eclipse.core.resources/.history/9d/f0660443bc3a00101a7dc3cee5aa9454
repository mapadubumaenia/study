package chap15.section02.example03;

@FunctionalInterface
interface MyScience{
	int score();
}
/**
 *  람다식2
 *
 */

public class S2_RamdaApplication {
public static void main(String[] args) {
//     인터페이스 변수=new 인터페이스(){}
	MyScience m=new MyScience() {
		
		@Override
		public int score() {
			// TODO Auto-generated method stub
			return 70;
		}
	};
//   메소드 실행
	System.out.println(m.score());
	
//  TODO 람다식
	MyScience m2=()->70;
	m2.score();
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}
