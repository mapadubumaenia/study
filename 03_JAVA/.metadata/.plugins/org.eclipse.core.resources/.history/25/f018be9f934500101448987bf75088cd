package codeup.section1;

import java.util.Scanner;

public class S1026 {
public static void main(String[] args) {
	
	Scanner sc=new Scanner(System.in);
	String a= sc.next();
	
	
	String b [] = a.split(":");
	
	
	int minute = Integer.parseInt(b[1]);  
    System.out.println(minute); 
	
	sc.close();
}
}
